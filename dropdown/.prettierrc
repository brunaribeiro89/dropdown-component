{
	"printWidth": 120,
	"singleQuote": true,
	"tabWidth": 2,
	"useTabs": false,
	"bracketSpacing": true,
	"bracketSameLine": true,
	"trailingComma": "es5",
  "semi": false,
	"arrowParens": "always",
	"proseWrap": "always",
	"embeddedLanguageFormatting": "auto",
	"organizeImportsSkipDestructiveCodeActions": true,
	"plugins": ["prettier-plugin-tailwindcss", "prettier-plugin-organize-imports"]	
}